{
/* The comics in this file tend to be grouped by provider. Because
of this, a given provider tends to get hit all at once. This causes
connections to fail even though the comic exists (throttling?). By
randomizing the comics, we lessen this problem.  */
/* "randomize": 1, */

/* For slow links it helps to limit the number of threads. This
helps reduce the number of timeouts. */
/* "threads": 4, */

"comics": [

/* ucomics.com */

{ /* Bound and Gagged */
  "url": "http://images.ucomics.com/comics/tmbou/%Y/tmbou%y%m%d.gif",
  "output": "bound-gagged"
},
{ /* Close to Home */
  "url": "http://images.ucomics.com/comics/cl/%Y/cl%y%m%d.gif"
},
{ /* Duplex */
  "url": "http://images.ucomics.com/comics/dp/%Y/dp%y%m%d.gif"
},
{ /* Free Range */
  "url": "http://www.gocomics.com/freerange/",
  "regexp": "http://.*\\.uclick\\.com/c2/[0-9a-f]+",
  "output": "free-range.gif" /* we need the extension */
},
{ /* Lio */
